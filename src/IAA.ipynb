    "labels2 = pd.read_csv(\"./annotate_eval/hoan.csv\", header=None)[0]\n",
    "\n",
    "kappa = cohen_kappa_score(labels1, labels2)\n",
    "\n",
    "print(\"Cohen's Kappa score:\", kappa)\n"
   ]
